<!DOCTYPE html>
<html>
<head>
  <title>¿Cómo Vamos?</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body class='<%= body_class %>'>
  <%= render 'header' %>

  <section id='content' class='wrapper'>
    <%= render 'alerts' %>
    <%= content_or_partial :sidebar unless full_layout? %>

    <div class="<%= content_class %>">
      <%= yield %>
    </div>
  </section>

  <%= render 'footer' %>
</body>
</html>
